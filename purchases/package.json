{
  "name": "certifications",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-files": "node -e \"var fs = require('fs'); process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true })); process.exit(0);\"",
    "clean": "npm run clean-files -- dist",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "serve:dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/server.ts",
    "serve:production": "node dist/server.js"
  },
  "author": "Felipe Ferrandin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
